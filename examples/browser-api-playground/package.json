{
  "name": "browser-api-playground",
  "version": "1.0.0",
  "description": "A playground for QA to test Search and Chat as separate pages with Glean Browser API.",
  "keywords": [],
  "main": "src/index.tsx",
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@gleanwork/web-sdk": "next",
    "antd": "^5.8.2",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "loader-utils": "3.2.1",
    "lodash": "^4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-json-view": "^1.21.3",
    "react-router": "6.9.0",
    "react-router-dom": "6.10.0",
    "react-scripts": "5.0.1",
    "react-virtualized-auto-sizer": "1.0.9"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.197",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "typescript": "4.4.2"
  },
  "scripts": {
    "start:client": "node webserver.js",
    "start:server": "node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "watch": "nodemon --watch src --ext js,jsx,ts,tsx,css --exec \"pnpm run build\"",
    "dev:client": "concurrently \"pnpm run watch\" \"pnpm run start:client\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
